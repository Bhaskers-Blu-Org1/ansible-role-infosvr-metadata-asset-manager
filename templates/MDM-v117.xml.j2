<ImportParameters bridgeDisplayName="IBM InfoSphere Master Data Management" bridgeId="FILE/MDM" bridgeVersion="1.0" release="{{ ibm_infosvr_metadata_asset_mgr_installed_version_full }}">
  <Parameter isRequired="true" displayName="XMI file" id="XMI file">
    <value></value>
  </Parameter>
  <Parameter displayName="If an asset description already exists" id="Asset_description_already_exists">
{% if item.item.existing_asset is defined and item.item.existing_asset == 'Keep_existing_description' %}
    <displayValue>Keep the existing description</displayValue>
    <value>{{ item.item.existing_asset }}</value>
{% else %}
    <displayValue>Replace the existing description</displayValue>
    <value>Replace_existing_description</value>
{% endif %}
  </Parameter>
</ImportParameters>
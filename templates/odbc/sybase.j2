
[{{ item.name }}]
Driver={{ ibm_infosvr_metadata_asset_mgr_install_location }}/Server/branded_odbc/lib/VMase00.so
Description={{ item.description | default('DataDirect Sybase Wire Protocol Driver') }}
ApplicationName={{ item.ApplicationName | default('') }}
ApplicationUsingThreads={{ item.ApplicationUsingThreads | default('1') }}
